<entityTemplates>
    
    <entityTemplate name="Tile">
        <component name="BodyRect" x="0.0" y="0.0" width="16.0" height="16.0" />
        <component name="RenderQuad" z="1" blocksLight="false"/>
        <component name="TextureRect" x="0" y="0" width="16" height="16" />
    </entityTemplate>
    
    <entityTemplate name="Player">
        <component name="Player" />
        <component name="BodyRect" x="0.0" y="0.0" width="20.0" height="20.0" />
        <component name="CharacterSpeed" speed="100" />
        <component name="Velocity" dx="0" dy="0" />
        <component name="Camera" priority="1" x="0" y="0" width="640" height="360" />
        <component name="AnimationData" firstStateName="left" delay="0.1" animationStatesFile="animations/character.animstates.xml"/>
        <component name="RenderQuad" textureFilepath="textures/characters/playerFullAnimation.png" z="95" blocksLight="false"/>
        <component name="TextureRect" x="0" y="0" width="0" height="0" />
    </entityTemplate>

</entityTemplates>

<entities>
    <entity sourceTemplate="Player">
    </entity>
</entities>
