        <component name="PointLight" offsetX="10" offsetY="10" r="255" g="255" b="255" range="50"/>
        <component name="Killable" />
        <component name="AnimationData" firstStateName="left" delay="0.1" animationStatesFile="animations/character.animstates.xml"/>
        <component name="ParticleEmitter">
            <particleAttrib name="startColor" r="0" g="10" b="100" a="255"/>
            <particleAttrib name="spawnPositionOffset" x="-500" y="-300"/>
            <particleAttrib name="randomSpawnAreaSize" width="1000" height="20"/>
            <particleAttrib name="initialVelocity" x="0" y="4"/>
            <particleAttrib name="acceleration" x="0.2" y="0.5"/>
            <particleAttrib name="size" x="6" y="6"/>
            <particleAttrib name="amount" v="450"/>
            <particleAttrib name="lifetime" v="3"/>
            <particleAttrib name="isEmitting" v="true"/>
        </component>
        <component name="MultiParticleEmitter"/>
    </entityTemplate>


